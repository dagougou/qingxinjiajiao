<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.it.qingxin.mapper.MoneyMapper">
    <select id="getMoneyByPhone" resultType="com.it.qingxin.bean.Money">
        select * from par_money where phone_num = #{phone};
    </select>
</mapper>